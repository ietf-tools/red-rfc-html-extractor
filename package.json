    "unpdf": "1.2.2",
    "zod": "3.25.30"
  },
  "devDependencies": {
    "@types/jsdom": "21.1.7",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.0.10",
    "@types/sanitize-html": "2.16.0",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  }
